{
  "name": "handlebars-helpers-underscore.string",
  "version": "0.1.0",
  "description": "Wrap underscore.string functions to Handlebars helpers",
  "main": "dist/handlebars-helpers-underscore.string.js",
  "scripts": {
    "build": "npm run build-js; npm run uglify",
    "build-js": "coffee --bare --compile --no-header --output dist src/handlebars-helpers-underscore.string.coffee",
    "uglify": "uglifyjs dist/handlebars-helpers-underscore.string.js --output dist/handlebars-helpers-underscore.string.min.js --compress --mangle",
    "test": "jasmine-node --coffee spec/",
    "coveralls": "ibrik cover jasmine-node --default-excludes --files.include=\"src/*.coffee\" -- --coffee spec/ && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomi77/handlebars-helpers-underscore.string.git"
  },
  "keywords": [
    "handlebars",
    "helpers",
    "underscore.string"
  ],
  "author": "Tomasz Jakub Rup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomi77/handlebars-helpers-underscore.string/issues"
  },
  "homepage": "https://github.com/tomi77/handlebars-helpers-underscore.string",
  "dependencies": {
    "handlebars": ">=2.0.0",
    "underscore.string": ">=3.2.0"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "coveralls": "^2.11.11",
    "istanbul": "^0.4.4",
    "jasmine-node": "^1.14.5",
    "uglify-js": "^2.7.0"
  }
}
